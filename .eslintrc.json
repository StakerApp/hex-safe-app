{
  "env": {
      "node": true,
      "es2021": true
  },
  "extends": [
      "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "ignorePatterns": [
      "node_modules/",
      "dist/"
  ],
  "rules": {
      "require-jsdoc": 0,
      "max-len": 0,
      "indent": [
          "error",
          4
      ],
      "object-curly-spacing": [
          "error",
          "always"
      ]
  }
}